def profil(nama, alamat, gender, umur, hobi):
    print("Data Diri")
    print("Nama\t:",nama)
    print("Alamat\t:", alamat)
    print("Gender\t:", gender)
    print("Umur\t:", umur, "Tahun")
    print("Hobi\t:", hobi)

profil("Idris", "Jl. Kayumannis I No.12 Perum Atsiri Permai", "Laki-laki", "18", "Nyanyi")    
